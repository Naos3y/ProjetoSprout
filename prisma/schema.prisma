generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Login {
  lid Int @id @default(autoincrement())
  email String @unique
  password String
}

model User {
  uid Int @id @default(autoincrement())
  perm String?
  utype String?
  uemployeenumber String?
  uname String?
  ufoto String?
  udirectreports String?
  ustartingdate String?
  upais String?
  ucidade String?
  urole String?
  useniority String?
  ugroup String?
}